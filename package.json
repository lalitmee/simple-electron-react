{
    "name": "simple-electron",
    "productName": "Simple Electron App",
    "version": "1.0.0",
    "description": "Simple Electron App",
    "main": "./app/main.js",
    "bin": {
        "wine": "./index.js"
    },
    "scripts": {
        "test": "node index.js",
        "start": "electron-forge start",
        "electron": "webpack",
        "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose",
        "package": "electron-forge package",
        "make": "electron-forge make"
    },
    "dependencies": {
        "axios": "^0.9.1",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.6.0",
        "babelify": "^8.0.0",
        "classnames": "^2.2.3",
        "css-loader": "^0.28.9",
        "electron-compile": "^6.4.2",
        "electron-packager": "^11.0.1",
        "electron-packager-dummy-wine": "^1.0.2",
        "electron-reload": "^0.2.0",
        "electron-squirrel-startup": "^1.0.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.6",
        "jquery": "^2.2.3",
        "postcss-loader": "^2.1.0",
        "react": "^0.14.8",
        "react-autocomplete": "^1.0.0-rc2",
        "react-dom": "^0.14.7",
        "react-sound": "^0.4.0",
        "resolve-url-loader": "^2.2.1",
        "semantic-ui-css": "^2.2.14",
        "semantic-ui-react": "^0.78.2",
        "soundmanager2": "^2.97.20150601-a",
        "style-loader": "^0.20.1",
        "url-loader": "^0.6.2"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "electron-forge": "^5.1.0",
        "electron-prebuilt-compile": "1.8.2",
        "webpack": "^3.11.0"
    },
    "config": {
        "forge": {
            "make_targets": {
                "win32": ["squirrel"],
                "darwin": ["zip"],
                "linux": ["deb", "rpm"]
            },
            "electronPackagerConfig": {
                "packageManager": "npm"
            },
            "electronWinstallerConfig": {
                "name": "simple_electron"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "Simple Electron App"
            }
        }
    }
}